{
  "name": "my-new-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npx prisma generate && vite dev",
    "build": "npx prisma generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "auth:init": "esbuild src/lib/server/auth.init.ts --outfile=./dist/auth.init.mjs --bundle --external:bcrypt --external:read --platform=node --format=esm && node ./dist/auth.init.mjs"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/kit": "~1.16",
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/read": "^0.0.29",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "prisma": "^5.2.0",
    "svelte": "^3.59.2",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.0",
    "fuzzysort": "^2.0.4",
    "nodemailer": "^6.9.1",
    "read": "^2.1.0",
    "readline-sync": "^1.4.10"
  }
}
